<?xml version="1.0" encoding="utf-8" ?>
<Plugin>
  <Name>UserMgr</Name><!--名称必须和项目名称的最后一个点号后面的单词相同 且不能重复-->
  <Author>fanjinlong</Author>
  <Description>用户管理功能模块</Description>
  <Functions>
    <Url></Url> <!--内容为空的时候区默认值：Controller/{action}/{id}-->

    <Function>
      <Name>AddUserController</Name>
      <NameSpace>Zhihe.SmartPlatform.UserMgr.Controllers</NameSpace>
      <Controller>AddUser</Controller>
      <Action>Index</Action>
    </Function>
    
    <Function>
      <Name>DeleteController</Name> <!--必须与Controllers 文件夹中的Controller类名字相同-->
      <NameSpace>Zhihe.SmartPlatform.UserMgr.Controllers</NameSpace>
      <Controller>Delete</Controller>
      <Action>Index</Action>
    </Function>
   
  </Functions>
</Plugin>